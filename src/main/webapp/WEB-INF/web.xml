<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>Signup</servlet-name>
		<display-name>Signup</display-name>
		<description></description>
		<servlet-class>com.sists.pages.Signup</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Otp</servlet-name>
		<display-name>Otp</display-name>
		<description></description>
		<servlet-class>com.sists.pages.Otp</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Login</servlet-name>
		<display-name>Login</display-name>
		<description></description>
		<servlet-class>com.sists.pages.Login</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Otp2</servlet-name>
		<display-name>Otp2</display-name>
		<description></description>
		<servlet-class>com.sists.pages.Otp2</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Menu</servlet-name>
		<display-name>Menu</display-name>
		<description></description>
		<servlet-class>com.sists.pages.Menu</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Hide</servlet-name>
		<display-name>Hide</display-name>
		<description></description>
		<servlet-class>com.sists.pages.Hide</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Show</servlet-name>
		<display-name>Show</display-name>
		<description></description>
		<servlet-class>com.sists.pages.Show</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Unhide</servlet-name>
		<display-name>Unhide</display-name>
		<description></description>
		<servlet-class>com.sists.pages.Unhide</servlet-class>
	</servlet>
	

	<servlet-mapping>
		<servlet-name>Signup</servlet-name>
		<url-pattern>/Signup</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Otp</servlet-name>
		<url-pattern>/verify</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/Login_account</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Otp2</servlet-name>
		<url-pattern>/Otp2_connect</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Menu</servlet-name>
		<url-pattern>/main_menu</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Hide</servlet-name>
		<url-pattern>/Hide_file</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Show</servlet-name>
		<url-pattern>/Show_data</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Unhide</servlet-name>
		<url-pattern>/Unhide_file</url-pattern>
	</servlet-mapping>
	


</web-app>
